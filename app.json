{
  "name": "Imitari",
  "description": "A lightweight ShareX-compatible image uploader server",
  "keywords": ["rust", "actix"],
  "buildpacks": [
    {
      "url": "https://github.com/emk/heroku-buildpack-rust"
    },
    {
      "url": "emk/rust"
    }
  ],
  "stack": "container",
  "env": {
    "BASE_URL": {
      "required": true,
      "value": "herokuapp.com"
    },
    "AUTH_TOKEN": {
      "required": true
    },
    "AUTH_USER": {
      "required": false
    },
    "URL": {
      "required": true,
       "value": "0.0.0.0"
    },
    "AUTH_PASSWORD": {
      "required": false
    },
    "NAME": {
      "required": true
    }
  }
}
